"""" Misc
set backspace=indent,eol,start

set hlsearch
set incsearch

set history=200
set showcmd

set number

"""" Visual Studio
nmap <leader>rl :source ~/.vsvimrc<CR>

nmap <leader>cp :vsc File.CopyFullPath<CR>
"nmap <leader>cr :vsc File.CopyFullPath<CR>

nmap <leader>rn :vsc Refactor.Rename<CR>

nmap <leader>gr :vsc Edit.FindAllReferences<CR>
nmap <leader>gf :vsc Edit.GotoBase<CR>
nmap <leader>gt :vsc Edit.GotoImplementations<CR>
nmap <leader>gh :vsc EditorContextMenus.CodeWindow.ToggleHeaderCodeFile<CR>

nmap gd :vsc Edit.GotoDefinition<CR>

"nmap <leader>rn :vsc ReSharper.ReSharper_Rename<CR>
"
"nmap gd :vsc ReSharper.ReSharper_GotoDeclaration<CR>
"nmap <leader>gr :vsc ReSharper.ReSharper_FindUsages<CR>
"nmap <leader>gf :vsc ReSharper.ReSharper_GotoBase<CR>
"nmap <leader>gt :vsc ReSharper.ReSharper_GotoImplementations<CR>
"
"nmap [g :vsc ReSharper.ReSharper_GotoPrevErrorInSolution<CR>
"nmap ]g :vsc ReSharper.ReSharper_GotoNextErrorInSolution<CR>
